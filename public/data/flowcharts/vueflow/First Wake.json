        },
        "type": "PlayerChoice",
        "data": {
          "fileId": "114921556394050178",
          "text": "没事了，一切都没事了",
          "targetBlock": "114391552895444453",
          "customMsgText": null,
          "customImgMsg": null,
          "additionalEvents": null,
          "itemId": 7
        }
      },
      {
        "id": "114391552895444453",
