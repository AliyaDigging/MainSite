      {
        "id": "114359682917236761",
        "position": {
          "x": 0,
          "y": 0
        },
        "type": "AliyaMessage",
        "data": {
          "fileId": "114359682917236761",
          "autoWaitTime": true,
          "waitSeconds": 1,
          "messageText": "感觉有一些胸闷呢\n咳咳，不对，我不是已经死了吗？\n为什么，我现在躺在病床上？\n{$name}，你还在吗？",
          "imageId": null,
          "itemId": 4
        }
      },
      {
        "id": "114871457982351770",
        "position": {
          "x": 0,
          "y": 0
        },
        "type": "PlayerChoice",
        "data": {
          "fileId": "114871457982351770",
          "text": "我在",
          "targetBlock": "114391552895444453",
          "customMsgText": null,
          "customImgMsg": null,
          "additionalEvents": null,
          "itemId": 6
        }
      },
      {
        "id": "114921556394050178",
        "position": {
          "x": 0,
